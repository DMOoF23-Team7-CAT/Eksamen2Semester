<UserControl x:Class="KlatreKongen.MVVM.View.OverviewView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ViewModel="clr-namespace:KlatreKongen.MVVM.ViewModel"
             xmlns:local="clr-namespace:KlatreKongen.MVVM.View"
             mc:Ignorable="d" 
             d:DesignHeight="704" d:DesignWidth="1346">

    <UserControl.DataContext>
        <ViewModel:OverviewViewModel/>
    </UserControl.DataContext>

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="449"/>
        </Grid.ColumnDefinitions>

        <Grid Grid.Column="0">
            <Border CornerRadius="20" Background="LightGray" Margin="20"/>
        </Grid>

        <Grid Grid.Column="1">
            <Border CornerRadius="20" Background="Gray" Margin="20"/>
        </Grid>

    </Grid>
</UserControl>
